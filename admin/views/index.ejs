<%- include('header') %>

  <div class="container">
    <div class="columns">
      <div class="column is-10">
        <div class="card main">
          <header class="card-header">
            <p class="card-header-title">Queues</p>
            <a href="/create" class="card-header-icon" aria-label="create">
              <span class="icon has-text-primary">
                <i class="fas fa-lg fa-plus-circle" aria-hidden="true"></i>
              </span>
            </a>
          </header>
          <div class="card-content">
            <div class="content">
              <table class="table is-striped">
                <tbody>
                  <% queues.forEach( queue => { %>
                    <tr>
                      <td><%= queue %></td>
                    </tr>
                  <% }); %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

<%- include('footer') %>
